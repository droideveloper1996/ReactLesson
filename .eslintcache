in} className=\"btn btn-primary\">\n          {\" \"}\n          Toggle Login{\" \"}\n        </button>\n\n        {this.state.isLoggedIn && (\n          <ConditionalRender loggedInStatus={this.state.isLoggedIn} />\n        )}\n      </main>\n    );\n  }\n}\n\n/**\n *\n * M V C  ---> Model View Controler\n *\n * CRUD  --> Create(POST) , Update(PUT), Delete(delete), Read (GET)\n *\n */\n\n// toolbar=[{title:'Top Offer',image:'www.xyz/com/sekjfgjks.png'},{title:Grocey,img\"';'},{},{},{}]\n\n// function abc(num) {\n// {title:'Top Offer',image:'www.xyz/com/sekjfgjks.png'}\n//   return <h2>{num * num}</h2>;\n// }\n\n/***\n *\n * constructor()\n * props\n * Data passing Through Component\n *\n * Class Component / Functional Component\n *\n * Navigation React\n * Netowork Calls\n * react-router-dom\n *\n * HTML  anchor Tag\n *\n * <a href=\"link\"/>\n * <Link href=\"link\"/>\n *\n *\n * Form --> Send(POST)\n * POST--->FETCH (GET)\n * UPDATE---> PUT\n * DELETE-->delete\n *\n * API Nodejs\n *\n */\n\n/**\n *\n * Conditional Rendering\n *\n * if(loggedIn === true){\n *   then render his name\n * }\n * else{\n *    show Login Button\n * }\n *\n *\n * constructor\n *\n * onMount\n *\n *\n */\n\nconst abc = (num) => <TestComponent />;\n\nexport default CounterComponent;\n","/Users/abhishekkushwaha/ReactLesson/src/components/LoginForm.jsx",[],"/Users/abhishekkushwaha/ReactLesson/src/components/xyz.jsx",[],"/Users/abhishekkushwaha/ReactLesson/src/components/navbar.jsx",["99"],"/Users/abhishekkushwaha/ReactLesson/src/redux/action/action.jsx",[],"/Users/abhishekkushwaha/ReactLesson/src/components/abc.jsx",[],"/Users/abhishekkushwaha/ReactLesson/src/redux/reducer/incrementReducer.jsx",[],"/Users/abhishekkushwaha/ReactLesson/src/redux/reducer/authReducer.jsx",[],"/Users/abhishekkushwaha/ReactLesson/src/components/test.jsx",[],"/Users/abhishekkushwaha/ReactLesson/src/components/class_Component.jsx",[],"/Users/abhishekkushwaha/ReactLesson/src/components/functional.jsx",["100"],"import R